@import url("basic.css");

/* -- page layout ----------------------------------------------------------- */

html {
    margin: 0px;
    padding: 0px;
}

body {
    line-height: 1.5;
    margin: auto;
    padding: 0px;
    background: #FFF url(bg_dotted.png);
    font-family: "Trebuchet MS", Tahoma, Helvetica, Arial, San-serif;
    min-width: 59em;
    max-width: 70em;
    color: #333333;
}

div.footer {
    padding: 8px;
    font-size: 11px;
    text-align: center;
    letter-spacing: 0.5px;
}

/* -- link colors and text decoration --------------------------------------- */

a:link {
    font-weight: bold;
    text-decoration: none;
}

a:visited {
    font-weight: bold;
    text-decoration: none;    
}

a:hover, a:active {
    font-weight: bold;
    text-decoration: underline;
}

/* -- Some headers act as anchors, don't give them a hover effect ----------- */

h1 a:hover, a:active {
    text-decoration: none;    
}

h2 a:hover, a:active {
    text-decoration: none;    
}

h3 a:hover, a:active {
    text-decoration: none;    
}

h4 a:hover, a:active {
    text-decoration: none;    
}

a.headerlink {
    padding-left: 5px;
}

a.headerlink:hover {
    color: #a7ce38;
}

/* -- basic text elements --------------------------------------------------- */

div.wrapper {
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-box-shadow: 0px 1px 2px #666;
    -khtml-box-shadow: 0px 1px 2px #666;
    -moz-box-shadow: 0px 1px 2px #666;
    margin: 20px 0px 20px 0px;
    padding: 20px 40px 50px 40px;
    background: #FFFCFF;
}

div.content {
    font-size: 0.9em;
}

/* -- heading and navigation ------------------------------------------------ */

div.header {
    -webkit-border-radius: 0px 0px 4px 4px;
    -khtml-border-radius: 0px 0px 4px 4px;
    -moz-border-radius: 0px 0px 4px 4px;
    border-radius: 0px 0px 4px 4px;
    -webkit-box-shadow: 0px 1px 2px #666;
    -khtml-box-shadow: 0px 1px 2px #666;
    -moz-box-shadow: 0px 1px 2px #666;
}

div.header-top {
    -webkit-box-shadow: 0px 1px 1px #fff inset;
    -khtml-box-shadow: 0px 1px 1px #fff inset;
    -moz-box-shadow: 0px 1px 1px #fff inset;
    position: relative;
    left: 0px;
    top: 0px;
    height: 85px;
    background: #F1F1F1;
    background: linear-gradient(top, #F1F1F1, #FFFFFF);
    background: -moz-linear-gradient(top, #F1F1F1, #FFFFFF);
    background: -webkit-gradient(linear, left top, left bottom, from(#F1F1F1), to(#FFFFFF));
    border-bottom: 1px solid #e7e7eb;
    padding: 0 40px;
}

div.header-top h1 {
    font-size: 1.6em;
    font-wieght: normal;
    letter-spacing: 1px;
    color: #0c3762;
    border: 0;
    margin: 0;
    padding-top: 15px;
}

div.header-top h1 a {
    font-decoration: normal;
    color: #0c3762;
}

div.header-top h2 {
    font-size: 1.3em;
    font-weight: normal;
    letter-spacing: 1px;
    text-transform: uppercase;
    color: #aaa;
    border: 0;
    margin-top: -3px;
    padding: 0;
}

div.header-top img.rightlogo {
    float: right;
}

div.title {
    font-size: 1.3em;
    font-weight: bold;
    color: #0c3762;
    border-bottom: dotted thin #e0e0e0;
    margin-bottom: 25px;
}

div.topnav {
    border-top: 1px solid #ffffff;
    background: #F2F2F2;
    background: linear-gradient(top, #F2F2F2, #E2E2E2);
    background: -moz-linear-gradient(top, #F2F2F2, #E2E2E2);
    background: -webkit-gradient(linear, left top, left bottom, from(#F2F2F2), to(#E2E2E2));
}

div.topnav p {
    margin-top: 0;
    margin-left: 40px;
    margin-right: 40px;
    margin-bottom: 0;
    text-align: right;
    font-size: 0.8em;
}

div.topnav ul {
    margin: 0;
    padding: 0;
    list-style-type: none;
    position: absolute;
}

div.topnav > ul li {
    display: none;
    position: relative;
    width: 300px;
    height: 38px;
    text-indent: 1.5em;
    background: #F2F2F2;
}

div.topnav ul li a {
    display: block;
    width: 100%;
    height: 100%;
    line-height: 38px;
    position: relative;
}

div.topnav ul li ul {
    display: none;
    position: absolute;
    top: 0;
    left: 300px;
}

/*
div.topnav ul:after {
    content: ".";
    display: block;
    clear: both;
    height: 0;
    visibility: hidden;
}
*/
div.header-middle {
    -webkit-bow-shadow: 0px 3px 2px #333 inset;
    -khtml-bow-shadow: 0px 3px 2px #333 inset;
    -moz-bow-shadow: 0px 3px 2px #333 inset;
    position: relative;
    top: 0px;
    left: 0px;
    margin: 0;
    padidng-bottom: 20px;
    background: linear-gradient(top, #CCC, #E7E7EB);
    background: -moz-linear-gradient(top, #CCC, #E7E7EB);
    background: -webkit-gradient(linear, left top, left bottom, from(#CCC), to(#E7E7EB));
    display: none;
}

div.header-middle div.globaltoc {
    margin: 0 40px;
    padding-top: 3px;
    padidng-bottom: 20px;
}

div.header-middle div.globaltoc h3 {
    margin: 0;
}

div.header-middle div.globaltoc ul {
    margin-bottom: 0;
}

div.bottomnav {
/*  background: #F2F2F2; */
}

div.bottomnav p {
    margin-top: 0px;
    margin-left: 40px;
    margin-right: 40px;
    margin-bottom: 0px;
    text-align: right;
    font-size: 0.8em;
}

a.uplink {
    font-weight: normal;
}


/* -- contents box ---------------------------------------------------------- */

table.index {
    margin: 0px 0px 30px 30px;
    padding: 1px;
    border-width: 1px;
    border-style: dotted;
    border-color: #e0e0e0;
}

table.index tf.heading {
    background-color: #e0e0e0;
    text-align: center;
    font-weight: bold;
    font-size: 1.1em;
}

table.index tr.index {
    background-color: #eeeeee;
}

table.index td {
    padding: 5px 20px;
}

table.index a:link, table.index a:visited {
    font-weight: normal;
    text-decoration: none;
    color: #dc3c01;
}

table.index a:hover, table.index a:active {
    text-decoration: underline;
    color: #ff4500;
}


/* -- dotted styles and layout -------------------------------------------- */

/* Rounded corner boxes */
