}


/* -- more layout styles ---------------------------------------------------- */

h1 {
    font-size: 1.3em;
    font-weight: bold;
    color: #333333;
    border-bottom: dotted thin #e0e0e0;
    margin-top: 30px;
}

h2 {
    font-size: 1.2em;
    font-weight: normal;
    color: #333333;
    border-bottom: dotted thin #e0e0e0;
    margin-top: 30px;
}

h3 {
    font-size: 1.1em;
    font-weight: normal;
    color: #333333;
    margin-top: 30px;
}

h4 {
    font-size: 1.0em;
    font-weight: normal;
    color: #333333;
    margin-top: 30px;
}

p {
    text-align: justify;
}

p.last {
    margin-bottom: 0;
}

ol {
    padding-left: 20px;
}

ul {
/*  padding-left: 5px; */
    margin-top: 3px;
}

li {
    line-height: 1.3;
}

div.content ul > li {
/*  -moz-background-clip: border;
    -moz-background-inline-policy: continuous;
    -moz-background-origin: padding;
    background: transparent url(bullet_orange.png) no-repeat scroll left 0.45em;
    list-style-image: none;
    list-style-type: none;
    padding: 0 0 0 1.666em; */
    margin-bottom: 3px;
}

td {
    vertical-align: top;
}

tt {
    background-color: #E2E2E2;
    font-size: 1.0em;
    font-family: monospace;
}

pre {
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    border-style: solid;
    border-width: thin;
    border-color: #0c3762;
    margin: 0 0 12px 0;
    padding: 0.8em;
    background-color: #F0F0F0;
}

.highlight {
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}

div.linenodiv {
    margin-top: 2px;
}

div.linenodiv pre {
    padding: 0.8em 0;
}

table.docutils thead {
    background: #F2F2F2;
    background: linear-gradient(top, #F2F2F2, #fff);
    background: -moz-linear-gradient(top, #F2F2F2, #fff);
    background: -webkit-gradient(linear, left top, left bottom, from(#F2F2F2), to(#fff));
}

hr {
    border-top: 1px solid #ccc;
    border-bottom: 0;
    border-right: 0;
    border-left: 0;
    margin-top: 20px;
    margin-bottom: 10px;
}

/* -- printer only pretty stuff ----- */
@media print {
    .noprint {
        display: none;
    }
    /* for acronyms we want their definitions inlined at print time */
    acronym[title]:after {
        font-size: small;
        content: " (" attr(title) ")";
        font-style: italic;
    }
    /* and not have mozilla dotted underline */
    acronym {
        border:none;
    }
    div.topnav, div.bottomnav, div.header, table.index {
        display: none;
    }
    div.content {
        margin: 0px;
        padding: 0px;
    }
    html {
        background: #FFF;
    }
}

