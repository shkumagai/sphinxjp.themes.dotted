}

div.seealso {
    background: #eaf4fc;
/*  background: linear-gradient(top, #eaf4fc, #eaf4fc 60%, #fff);
    background: -moz-linear-gradient(top, #eaf4fc, #eaf4fc 60%, #fff);
    background: -webkit-gradient(linear, left top, left bottom,
                                 from(#eaf4fc), color-stop(0.6, #eaf4fc), to(#fff));
*/
